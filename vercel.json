json{
  "build": {
    "env": {
      "HUGO_VERSION": "0.119.0"
    }
  },
  "buildCommand": "hugo --gc --minify",
  "outputDirectory": "public",
  "installCommand": "curl -L -O https://github.com/gohugoio/hugo/releases/download/v${HUGO_VERSION}/hugo_extended_${HUGO_VERSION}_Linux-64bit.tar.gz && tar -xzf hugo_extended_${HUGO_VERSION}_Linux-64bit.tar.gz && sudo mv hugo /usr/local/bin/",
  "framework": null,
  "redirects": [
    {
      "source": "/admin",
      "destination": "/admin/index.html"
    }
  ]
}